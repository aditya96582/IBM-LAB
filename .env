# Samadhan AI Frontend Environment Configuration
# Basic configuration to get the project running

# Firebase Configuration (using demo values - replace with your actual Firebase config)
VITE_FIREBASE_API_KEY=demo-api-key
VITE_FIREBASE_AUTH_DOMAIN=samadhan-ai-demo.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=samadhan-ai-demo
VITE_FIREBASE_STORAGE_BUCKET=samadhan-ai-demo.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:demo123456

# Google Gemini AI API (optional)
VITE_GEMINI_API_KEY=demo-gemini-key

# ElevenLabs API (optional)
VITE_ELEVENLABS_API_KEY=demo-elevenlabs-key

# Backend URL
VITE_BACKEND_URL=http://localhost:5000

# Application Configuration
VITE_APP_NAME=Samadhan AI
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development

# Feature Flags
VITE_ENABLE_VOICE_CHAT=false
VITE_ENABLE_SPEECH_SYNTHESIS=false
VITE_ENABLE_FIREBASE_ANALYTICS=false
VITE_DISABLE_FIREBASE=true
VITE_USE_LOCAL_STORAGE=true

# Development Settings
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=info